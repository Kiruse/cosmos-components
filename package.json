{
  "name": "@kiruse/cosmos-components",
  "type": "module",
  "version": "0.1.4",
  "description": "Kiru's Cosmos Web3 Components",
  "keywords": [
    "cosmos",
    "web3",
    "components",
    "web components",
    "blockchain",
    "dapp",
    "frontend",
    "vanilla",
    "esm"
  ],
  "author": "Kiruse",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*"
  },
  "scripts": {
    "dev": "run-p dev:*",
    "dev:components": "bun run dev.coffee",

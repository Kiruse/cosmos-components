    "@preact/signals": "^2.0.1",
    "happy-dom": "^17.4.4",
    "preact": "^10.26.4",
    "qrcode": "^1.5.4",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@apophis-sdk/cosmos-signers": "^0.3.0",
    "@babel/generator": "^7.26.10",
    "@babel/traverse": "^7.26.10",
    "@chromatic-com/storybook": "^3.2.5",
    "@storybook/addon-essentials": "^8.6.4",
    "@storybook/blocks": "^8.6.4",
    "@storybook/experimental-addon-test": "^8.6.4",
    "@storybook/test": "^8.6.4",
    "@storybook/web-components-vite": "^8.6.4",
    "@types/babel__generator": "^7.6.8",
    "@types/babel__traverse": "^7.20.6",
    "@types/bun": "latest",
    "@types/picomatch": "^3.0.2",
    "@types/pug": "^2.0.10",
    "@types/qrcode": "^1.5.5",
    "bun-coffeescript": "^1.0.3",
    "chokidar": "^4.0.3",
    "npm-run-all2": "^7.0.2",
    "picomatch": "^4.0.2",
    "pug": "^3.0.3",
    "storybook": "^8.6.4"
  },
  "peerDependencies": {
    "@apophis-sdk/core": "^0.3.0",
    "@apophis-sdk/cosmos": "^0.3.0",
    "@apophis-sdk/cosmwasm": "^0.3.0",
